if story.Likes && story.Likes.filter(like => like.userId === userId).length
    button.likeButton(class=`liked` id=`${story.id}`)
        <i class="fas fa-hand-sparkles"></i>
        span(id=`likeCount-${story.id}`) #{story.Likes.length}
else if !story.Likes
    button.likeButton(id=`${story.id}`)
        <i class="fas fa-hand-sparkles"></i>
        span(id=`likeCount-${story.id}`)  0
else
    button.likeButton(id=`${story.id}`)
        <i class="fas fa-hand-sparkles"></i>
        span(id=`likeCount-${story.id}`) #{story.Likes.length}
